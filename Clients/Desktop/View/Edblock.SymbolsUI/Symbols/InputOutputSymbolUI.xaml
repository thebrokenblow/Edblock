<UserControl
    x:Class="Edblock.SymbolsUI.Symbols.InputOutputSymbolUI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="80"
    d:DesignWidth="360"
    mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Canvas
            Width="140"
            Height="60"
            Margin="10,10,10,10">
            <Polygon Fill="#FF008080" Points="20 0 0 60 120 60 140 0" />

            <TextBlock
                Width="140"
                Height="60"
                Style="{StaticResource TextFieldUI}"
                Text="Ввод / Вывод" />
        </Canvas>

        <Label Content="Ввод / Вывод (6)" Style="{StaticResource LabelSymbolMenu}" />
    </StackPanel>
</UserControl>
