<UserControl
    x:Class="Edblock.SymbolsUI.Symbols.StartEndSymbolUI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="80"
    d:DesignWidth="300"
    mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Canvas
            Width="140"
            Height="60"
            Margin="10,10,10,10">

            <Rectangle
                Width="140"
                Height="60"
                Fill="#FFF25252"
                RadiusX="20"
                RadiusY="20" />

            <TextBlock
                Width="140"
                Height="60"
                Style="{StaticResource TextFieldUI}"
                Text="Начало / Конец" />
        </Canvas>

        <Label Content="Начало / Конец (5)" Style="{StaticResource LabelSymbolMenu}" />

    </StackPanel>
</UserControl>
