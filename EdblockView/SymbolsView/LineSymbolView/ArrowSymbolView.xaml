<UserControl
    x:Class="EdblockView.SymbolsView.LineSymbolView.ArrowSymbolView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:lineSymbolVM="clr-namespace:EdblockViewModel.Symbols.LineSymbols;assembly=EdblockViewModel"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DataContext="{d:DesignInstance Type=lineSymbolVM:DrawnLineSymbolVM}"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Polygon
        DataContext="{Binding ArrowSymbol}"
        Points="{Binding PointArrowSymbol}"
        Style="{StaticResource ArrowSymbolView}" />

</UserControl>