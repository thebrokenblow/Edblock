<UserControl x:Class="EdblockView.SymbolsView.LineSymbol.ArrowSymbolView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:symbolsLineVM="clr-namespace:EdblockViewModel.Symbols.LineSymbols;assembly=EdblockViewModel" 
             d:DataContext="{d:DesignInstance Type=symbolsLineVM:DrawnLineSymbolVM}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Polygon 
            DataContext="{Binding ArrowSymbol}"
            Points="{Binding PointArrowSymbol}" 
            Style="{StaticResource ArrowSymbolView}"/>
    
</UserControl>
