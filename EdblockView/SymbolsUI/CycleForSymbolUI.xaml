<UserControl x:Class="EdblockView.SymbolsUI.CycleForSymbolUI"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EdblockView.SymbolsUI"
             mc:Ignorable="d" 
             d:DesignHeight="60" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <Canvas            
            Width="140" 
            Height="60">
            <Polygon 
                Fill="#FFC618" 
                Points="10 0 0 10 0 50 10 60 130 60 140 50 140 10 130 0"/>

            <TextBlock 
                Width="140"
                Height="60"
                Style="{StaticResource TextFieldUI}"
                Text="Цикл for"/>
        </Canvas>
        
        <Label
            Margin="10 0 0 0"
            Style="{StaticResource LabelSymbolMenu}"
            Content="Цикл (8) for" />
    </StackPanel>
</UserControl>
