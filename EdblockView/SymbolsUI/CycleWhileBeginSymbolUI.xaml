<UserControl
    x:Class="EdblockView.SymbolsUI.CycleWhileBeginSymbolUI"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="80"
    d:DesignWidth="350"
    mc:Ignorable="d">
    <StackPanel Orientation="Horizontal">
        <Canvas
            Width="140"
            Height="60"
            Margin="10,10,10,10">
            <Polygon Fill="#CCCCFF" Points="0 60 0 10 10 0 130 0 140 10 140 60" />

            <TextBlock
                Width="140"
                Height="60"
                Style="{StaticResource TextFieldUI}"
                Text="Начало цикла" />
        </Canvas>

        <Label Content="Цикл (9) do while, while (Начало)" Style="{StaticResource LabelSymbolMenu}" />
    </StackPanel>
</UserControl>