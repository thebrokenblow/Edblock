<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="FrameSymbol" TargetType="Canvas">
        <Setter Property="Width" Value="140" />
        <Setter Property="Height" Value="60" />
    </Style>

    <Style x:Key="TextFrameSymbol" TargetType="TextBox">
        <Setter Property="Width" Value="140" />
        <Setter Property="Height" Value="60" />
        <Setter Property="Cursor" Value="{Binding TextField.Cursor}" />
        <Setter Property="TextAlignment" Value="Center" />
        <Setter Property="Focusable" Value="{Binding TextField.Focus}" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Foreground" Value="White" />
    </Style>

    <Style x:Key="ConnectionPoint" TargetType="Ellipse">
        <Setter Property="Width" Value="{Binding Diameter}" />
        <Setter Property="Height" Value="{Binding Diameter}" />
        <Setter Property="Fill" Value="{Binding Fill}" />
        <Setter Property="Stroke" Value="{Binding Stroke}" />
        <Setter Property="StrokeThickness" Value="1.5" />
    </Style>

</ResourceDictionary>