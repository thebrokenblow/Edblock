<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="TextFieldUI" TargetType="TextBlock">
        <Setter Property="Foreground" Value="White" />
        <Setter Property="TextAlignment" Value="Center" />
    </Style>

    <Style x:Key="LineCommentSymbolUI" TargetType="Line">
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stroke" Value="Black"/>
    </Style>

    <Style x:Key="LineConditionSymbolUI" TargetType="Line">
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stroke" Value="Black"/>
    </Style>

    <Style x:Key="LineParallelActionSymbolUI" TargetType="Line">
        <Setter Property="StrokeThickness" Value="1"/>
        <Setter Property="Stroke" Value="Black"/>
    </Style>

    <Style x:Key="ConnectionPointUI" TargetType="Ellipse">
        <Setter Property="Fill" Value="Black"/>
        <Setter Property="Width" Value="8"/>
        <Setter Property="Height" Value="8"/>
    </Style>

</ResourceDictionary>